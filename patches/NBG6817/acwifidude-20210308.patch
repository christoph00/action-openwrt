+ 					return NULL;
+ 				}
+ 
+-				DEBUG_TRACE("Have a gw address " ECM_IP_ADDR_OCTAL_FMT "\n", ECM_IP_ADDR_TO_OCTAL(gw_addr));
++				/*
++				 * The found gateway address can be all zeros,
++				 * so in this case use the host address.
++				 */
